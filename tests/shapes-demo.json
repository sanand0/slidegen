{
  "id": "shapes-demo",
  "name": "SVG Shapes Demo",
  "slide_size": { "w": 1280, "h": 720, "unit": "px" },
  "theme": {
    "colors": {
      "accent1": "#2563eb",
      "accent2": "#16a34a",
      "accent3": "#dc2626",
      "accent4": "#ca8a04",
      "text": "#0f172a",
      "bg": "#ffffff"
    },
    "fonts": {
      "heading": "Inter",
      "body": "Inter"
    }
  },
  "masters": [{
    "id": "demo-master",
    "bg": { "fill": "#f8fafc" },
    "shapes": [{
      "id": "title",
      "type": "text",
      "x": 2, "y": 1, "w": 96, "h": 5, "unit": "%",
      "text": "{slide_title}",
      "font_family": "Inter",
      "font_size": 24,
      "font_weight": 700,
      "color": "#0f172a",
      "text_align": "center"
    }],
    "layouts": [{
      "id": "shape-showcase",
      "shapes": []
    }]
  }],
  "slides": [
    {
      "id": "basic-shapes",
      "master": "demo-master",
      "layout": "shape-showcase",
      "meta": { "slide_title": "Basic Shapes - Rectangle, Rounded Rectangle, Ellipse, Diamond" },
      "shapes": [
        {"id": "rect1", "type": "rectangle", "x": 5, "y": 15, "w": 18, "h": 12, "unit": "%", "fill": "#2563eb", "text": "Rectangle", "text_color": "#ffffff", "text_size": 12},
